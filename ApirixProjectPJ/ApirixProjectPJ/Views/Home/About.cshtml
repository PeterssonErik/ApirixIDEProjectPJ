@{
    ViewData["Title"] = "About";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>ACE Autocompletion demo</title>
</head>
<body>
    <div class="dropdown">
        <button onclick="myFunction()" class="dropbtn">Färgschema</button>
        <div id="myDropdown" class="dropdown-content">
            <a onclick="white()" class="buttondiv"><p class="p-aligncenter">white</p></a>
            <a onclick="black()" class="buttondiv"><p class="p-aligncenter">black</p></a>
            <a onclick="grayblue()" class="buttondiv"><p class="p-aligncenter">grayblue</p></a>
        </div>
    </div>
    <h1 id="DocumentHeader">Text editor</h1>
    <pre id="editor"></pre>

    


    <!-- load ace -->
    <script src="../../no-conflikt/src-noconflict/ace.js"></script>
    <!-- load ace language tools -->
    <script src="../../no-conflikt/src-noconflict/ext-language_tools.js"></script>
    <!-- load external js file -->
    <script src="../../Scripts/documentjs.js"></script>
    <!-- load background changer -->
    <script src="../../Scripts/myBackground.js"></script>
</body>
</html>